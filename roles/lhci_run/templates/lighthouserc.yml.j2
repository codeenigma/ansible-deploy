ci:
  collect:
    url:
{% for url in lhci_run.test_urls %}
      - {{ url }}
{% endfor %}
    settings:
      chromeFlags:
        - "--no-sandbox"
        - "--ignore-certificate-errors"
      output: "html"
    numberOfRuns: 1
    headful: true
  upload:
    outputDir: "./reports"
    target: "filesystem"
